{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "redirects": [
    {
      "source": "/signin",
      "destination": "/auth/signin",
      "permanent": false
    },
    {
      "source": "/login",
      "destination": "/auth/signin",
      "permanent": false
    }
  ]
}
